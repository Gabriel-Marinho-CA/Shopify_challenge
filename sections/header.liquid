<header>
  <div class="wrapper-top-bar-header">
    <div class="top_bar">
      <div class="top_bar_content">
        <a href="#">
          <img src="{{section.settings.Ico_franqueado | image_url: width:14 | image_tag}}" alt="">
          <p>{{ section.settings.Franqueado }}</p>
        </a>
      </div>
      <div class="top_bar_content">
        <a href="#">
          <img src="{{section.settings.Ico_nossas_lojas | image_url: width:14 | image_tag}}" alt="">
          <p>{{ section.settings.Nossas-lojas }}</p>
        </a>
      </div>
    </div>
  </div>
</header>

{% schema %}
  {
    "name": "Top bar",
    "settings": [
      {
        "type": "text",
        "id": "Franqueado",
        "label": "Top bar | Franqueado",
        "default": "Seja um franqueado"
      }, {
        "type": "image_picker",
        "id": "Ico_franqueado",
        "label": "Image"
      }, {
        "type": "text",
        "id": "Nossas-lojas",
        "label": "Nossas lojas | texto",
        "default": "Nossas Lojas"
      }, {
        "type": "image_picker",
        "id": "Ico_nossas_lojas",
        "label": "Image"
      }
    ]
  }
{% endschema %}