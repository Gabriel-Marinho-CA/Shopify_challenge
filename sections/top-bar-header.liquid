<div class="wrapper-top-bar-header">
  <div class="top_bar container">
    <div class="top_bar_content">
      <a href="#">
        <img style="width:20px" src="{{ 'house-circle-check-solid.svg' | asset_url }}">
        <p>{{ section.settings.Franqueado }}</p>
      </a>
    </div>
    <div class="top_bar_content">
      <a href="#">
        <img style="width:15px" src="{{ 'location.svg' | asset_url }}">
        <p>{{ section.settings.Nossas-lojas }}</p>
      </a>
    </div>
  </div>
</div>
{% schema %}
  {
    "name": "Top bar",
    "settings": [
      {
        "type": "text",
        "id": "Franqueado",
        "label": "Top bar | Franqueado",
        "default": "Seja um franqueado"
      }, {
        "type": "text",
        "id": "Nossas-lojas",
        "label": "Nossas lojas | texto",
        "default": "Nossas Lojas"
      }
    ]
  }
{% endschema %}